flexbox()
	display flex
	display -webkit-box
	display -ms-flexbox
	display -webkit-flex

justify-content(value)
	justify-content value
	-webkit-justify-content value
	-webkit-box-pack value
	-ms-flex-pack value

align-items(value)
	align-items value
	-webkit-align-items value
	-webkit-box-align value
	-ms-flex-align value
	-webkit-align-items value

flex-direction(value)
	-webkit-flex-direction value
	flex-direction value

flex-direction(value)
	-webkit-box-direction value
	-webkit-flex-direction value
	flex-direction value

flex-wrap(value)
	flex-wrap value
	-ms-flex-wrap value
	-webkit-flex-wrap value

animate(time = .25s)
	-webkit-transition all time ease
	-moz-transition all time ease
	-ms-transition all time ease
	-o-transition all time ease
	transition all time ease
	
snipRotate(degree = 0deg)
	-webkit-transform rotate(degree)
	-moz-transform rotate(degree)
	-ms-transform rotate(degree)
	-o-transform rotate(degree)
	transform rotate(degree)

snipScale(scale = 1)
	-webkit-transform: scale(scale)
	-moz-transform: scale(scale)
	-o-transform: scale(scale)
	-ms-transform: scale(scale)
	
centering(maxWidth, margin=0)
	width 100%
	max-width maxWidth px
	margin margin auto
	padding-left 20px
	padding-right 20px

size(width, height)
	width width
	height height

max-screen(breakPoint)
	@media screen and (max-width: breakPoint px)
		{block}

min-screen(breakPoint)
	@media screen and (min-width: breakPoint px)
		{block}

screen(breakPointMin, breakPointMax)
	@media screen and (min-width: breakPointMin px) and (max-width: breakPointMax px)
		{block}